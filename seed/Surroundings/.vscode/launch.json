{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Cortex Debug",
          "type": "cortex-debug",
          "request": "launch",
          "servertype": "openocd",
          "cwd": "${workspaceRoot}",
          "executable": "${workspaceRoot}/build/Surroundings.elf",
          "device": "STM32H750",
          "configFiles": [
              "interface/stlink.cfg",
              "target/stm32h7x.cfg"
          ],
          "svdFile": "${workspaceRoot}/STM32H750.svd",
          "showDevDebugOutput": "raw",
          "preLaunchTask": "build",
          "postRestartCommands": [
              "monitor reset halt",
              "monitor reset init"
          ]
      }
  ]
}